Proceso Algoritmo
	Escribir 'Algoritmo que ejecuta operaciones basicas matematicas de 2 números'
	Escribir 'Digite el primer número a sumar'
	Leer a
	Escribir 'Digite el segundo número'
	Leer b
	R<-a+b
	M<-a-b
	N<-a*b
	P<-a/b
	Escribir "El resultado de la suma es:", R
	Escribir "El resultado de la resta es:",M
	Escribir "El resultado de la multiplicacion es: ", N
	Escribir "El resultado de la division es: " P
FinProceso

